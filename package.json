{
  "name": "css-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prisma:generate": "pnpm prisma generate",
    "prisma:migrate": "pnpm prisma migrate deploy",
    "prisma:seed": "pnpm prisma db seed",
    "start:docker": "pnpm run prisma:generate && pnpm run prisma:migrate && pnpm run prisma:seed && pnpm next build && pnpm next start",
    "preinstall": "npx only-allow pnpm",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "prisma": {
    "seed": "./node_modules/.bin/ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "prettier": {
    "bracketSpacing": true,
    "htmlWhitespaceSensitivity": "css",
    "jsxBracketSameLine": false,
    "jsxSingleQuote": false,
    "printWidth": 100,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "insertPragma": false,
    "requirePragma": false,
    "semi": true,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "es5",
    "rangeStart": 0,
    "bracketSameLine": true
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.8",
    "@fullcalendar/daygrid": "^6.1.8",
    "@fullcalendar/list": "^6.1.8",
    "@fullcalendar/react": "^6.1.8",
    "@hookform/resolvers": "^3.3.1",
    "@icons-pack/react-simple-icons": "^9.0.0",
    "@internationalized/date": "^3.5.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.2.0",
    "@radix-ui/react-alert-dialog": "^1.0.4",
    "@radix-ui/react-avatar": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-hover-card": "^1.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "^1.1.3",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.4",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "^20.5.7",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.15",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.15",
    "framer-motion": "^10.16.1",
    "lossless-json": "^2.0.11",
    "lucide-react": "^0.268.0",
    "next": "13.4.15",
    "next-auth": "^4.23.1",
    "next-themes": "^0.2.1",
    "postcss": "8.4.27",
    "prisma": "^5.2.0",
    "react": "18.2.0",
    "react-aria": "^3.28.0",
    "react-day-picker": "^8.8.1",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.46.1",
    "react-markdown": "^8.0.7",
    "react-remove-scroll-bar": "^2.3.4",
    "react-stately": "^3.26.0",
    "remark-gfm": "^3.0.1",
    "sharp": "^0.32.5",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "3.3.3",
    "tailwindcss-animate": "^1.0.6",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "zod": "^3.22.2"
  }
}
