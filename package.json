{
  "name": "css-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prisma:generate": "pnpm prisma generate",
    "prisma:migrate": "pnpm prisma migrate deploy",
    "prisma:seed": "pnpm prisma db seed",
    "start:docker": "pnpm run prisma:generate && pnpm run prisma:migrate && pnpm run prisma:seed && pnpm next build && pnpm next start",
    "preinstall": "npx only-allow pnpm",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "prisma": {
    "seed": "./node_modules/.bin/ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "prettier": {
    "bracketSpacing": true,
    "htmlWhitespaceSensitivity": "css",
    "jsxBracketSameLine": false,
    "jsxSingleQuote": false,
    "printWidth": 100,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "insertPragma": false,
    "requirePragma": false,
    "semi": true,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "es5",
    "rangeStart": 0,
    "bracketSameLine": true
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.10",
    "@fullcalendar/daygrid": "^6.1.10",
    "@fullcalendar/list": "^6.1.10",
    "@fullcalendar/react": "^6.1.10",
    "@hookform/resolvers": "^3.3.4",
    "@icons-pack/react-simple-icons": "^9.3.0",
    "@internationalized/date": "^3.5.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.8.1",
    "@radix-ui/react-alert-dialog": "^1.0.4",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-hover-card": "^1.0.7",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "10.4.17",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "framer-motion": "^10.18.0",
    "lossless-json": "^4.0.1",
    "lucide-react": "^0.312.0",
    "next": "14.1.0",
    "next-auth": "^4.24.5",
    "next-themes": "^0.2.1",
    "nextjs-toploader": "^1.6.6",
    "postcss": "8.4.33",
    "prisma": "^5.8.1",
    "react": "18.2.0",
    "react-aria": "^3.31.1",
    "react-day-picker": "^8.10.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.49.3",
    "react-markdown": "^9.0.1",
    "react-remove-scroll-bar": "^2.3.4",
    "react-stately": "^3.29.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "sharp": "^0.33.2",
    "tailwind-merge": "^2.2.0",
    "tailwindcss": "3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "zod": "^3.22.4"
  }
}
